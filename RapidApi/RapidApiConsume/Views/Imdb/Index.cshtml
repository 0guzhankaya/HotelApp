@model List<ApiMovieViewModel>
@{
	Layout = null;
}

@if (Model != null && Model.Any())
{
    <table class="table table-bordered">
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.rank</td>
                <td>@item.title</td>
                <td>@item.rating</td>
                <td><a href="#" class="btn btn-dark">Fragmanı Oynat</a></td>
            </tr>
        }
    </table>
}
else
{
    <p>No movies available.</p>
}
>